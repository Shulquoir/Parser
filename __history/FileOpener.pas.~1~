unit uFileOpener;

interface

uses
  Dialogs, Classes, SysUtils, StdCtrls;

implementation

type
  TFileOpener = class
  public
    class function OpenDFNFile(var ePathDFN: TEdit; var OriginalDFNFileContent: TStringList;
